{
  "compilerOptions": {
    "outDir": "./dist",
    "allowJs": true,
    "target": "es5",
    "alwaysStrict": true,
    "jsx": "react",
    "moduleResolution": "node",
    "module": "esnext",
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./src/*"]
    },
    // "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
  },
  "include": [
    "./src/**/*"
  ],
  "extends": "expo/tsconfig.base"
}
